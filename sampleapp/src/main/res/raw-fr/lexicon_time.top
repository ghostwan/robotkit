def:sayDate($1, $2, $3)
^first[
"$1-$2-$3==^currentDate aujourd'hui"
"$var=^currentDate+0000-00-01 $var==$1-$2-$3 demain"
"$1-$2-$3==now-now-sunday dimanche"
"$1-$2-$3==now-now-monday lundi"
"$1-$2-$3==now-now-tuesday mardi"
"$1-$2-$3==now-now-wednesday mercredi"
"$1-$2-$3==now-now-thursday jeudi"
"$1-$2-$3==now-now-friday vendredi"
"$1-$2-$3==now-now-saturday samedi"
"$2==01 le $3 janvier $1"
"$2==02 le $3 février $1"
"$2==03 le $3 mars $1"
"$2==04 le $3 avril $1"
"$2==05 le $3 mai $1"
"$2==06 le $3 juin $1"
"$2==07 le $3 juillet $1"
"$2==08 le $3 août $1"
"$2==09 le $3 septembre $1"
"$2==10 le $3 octobre $1"
"$2==11 le $3 novembre $1"
"$2==12 le $3 décembre $1"]

def:sayDateTime($1, $2, $3, $4, $5)
^first[
"$1-$2-$3 $4:$5==^currentDateTime maintenant"
"$1-$2-$3==^currentDate aujourd'hui à $4:$5"
"$var=^currentDate+0000-00-01 $var==$1-$2-$3 demain à $4:$5"
"$1-$2-$3==now-now-sunday dimanche à $4:$5"
"$1-$2-$3==now-now-monday lundi à $4:$5"
"$1-$2-$3==now-now-tuesday mardi à $4:$5"
